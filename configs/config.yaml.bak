# config
debug: true
cron: false
env: dev

domain:
  h5: "https://www.omoai.com"

server:
  rpc_port: 10132
  http_port: 10131
  monitor_port: 10133

database:
  debug: true
  db_conf:
    conn_max_life_time: 60
    max_idle_conn: 60
    max_open_conn: 10
  default:
    driver: mysql
    source: root:123456@tcp(127.0.0.1:3306)/omiai?charset=utf8mb4&parseTime=True&loc=Local

cache:
  driver: redis
  url: redis://:123456@127.0.0.1:6379/1?dial_timeout=3&read_timeout=6s&max_retries=2

redis:
  default:
    url: redis://:123456@127.0.0.1:6379/1?dial_timeout=3&read_timeout=6s&max_retries=2
    host: r-2zecgwl3v7o30l1tcp.redis.rds.aliyuncs.com
    port: 6379
    password: "123456"

runtime:
  path: /Users/edy/apps/go/src/github.com/iwuxc/omiai-server/runtime/
  download: /Users/edy/apps/go/src/github.com/iwuxc/omiai-server/runtime/download/

log:
  path: /Users/edy/apps/go/src/github.com/iwuxc/omiai-server/runtime/logs/           # 本地日志路径 1
  level: debug                  # 日志级别 [panic, fatal, error, warn, info, debug, trace]
  max_age: 30                  # 日志最大保留时长：100天

#  队列重试配置
queue:
  retry: 2
  timeout: 600
  retention: 86400
  concurrency: 20

track:
  path: /Users/edy/apps/go/src/github.com/iwuxc/omiai-server/runtime/logs/           # 本地日志路径
  max_age: 1                  # 日志最大保留时长：100天

cron_conf:
  host: 127.0.0.1
  port: 6379
  password: "123456"  # 这个密码不能 encode
  db: 2 #这个库不能和其他服务的定时库一样
  cron_name: omiai-server-dev